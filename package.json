{
  "name": "@josempgon/vue-keycloak",
  "version": "1.5.0",
  "description": "Keycloak plugin for Vue3 and Composition API",
  "author": {
    "name": "Gery Hirschfeld",
    "email": "gerhard.hirschfeld@baloise.ch",
    "url": "https://github.com/hirsch88"
  },
  "homepage": "https://github.com/JoseGoncalves/vue-keycloak",
  "repository": {
    "type": "git",
    "url": "git@github.com:JoseGoncalves/vue-keycloak.git"
  },
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watchAll",
    "build": "npm run build:clean && npm run build:compile && npm run build:bundle",
    "build:clean": "rimraf dist && rimraf dist-transpiled",
    "build:compile": "tsc -p .",
    "build:bundle": "rollup --config rollup.config.js",
    "lint": "eslint src --ext .ts,vue",
    "format": "npm run prettier:write",
    "prettier:write": "prettier --write \"./src\"",
    "prettier:check": "prettier --check \"./src\"",
    "release": "semantic-release"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.esm.js",
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist/"
  ],
  "keywords": [
    "vue",
    "keycloak",
    "composition-api"
  ],
  "license": "Apache-2.0",
  "peerDependencies": {
    "jwt-decode": "^3.1.2",
    "keycloak-js": "^15.0.2",
    "vue": "^3.0.8"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.1",
    "@semantic-release/github": "^7.2.3",
    "@semantic-release/npm": "^7.1.0",
    "@types/jest": "^27.0.2",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "eslint": "^7.32.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-vue": "^7.20.0",
    "jest": "^27.3.1",
    "jwt-decode": "^3.1.2",
    "keycloak-js": "^15.0.2",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.58.0",
    "semantic-release": "^17.4.7",
    "ts-jest": "^27.0.7",
    "typescript": "^4.4.4",
    "vue": "^3.2.20"
  }
}
