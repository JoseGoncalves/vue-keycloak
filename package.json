{
  "name": "@josempgon/vue-keycloak",
  "version": "2.1.0",
  "description": "Keycloak plugin for Vue3 and Composition API",
  "author": {
    "name": "Gery Hirschfeld",
    "email": "gerhard.hirschfeld@baloise.ch",
    "url": "https://github.com/hirsch88"
  },
  "contributors": [
    "José Miguel Gonçalves <jose.goncalves@inov.pt>"
  ],
  "homepage": "https://github.com/JoseGoncalves/vue-keycloak",
  "repository": {
    "type": "git",
    "url": "git@github.com:JoseGoncalves/vue-keycloak.git"
  },
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watchAll",
    "build": "npm run build:clean && npm run build:compile && npm run build:bundle",
    "build:clean": "rimraf dist && rimraf dist-transpiled",
    "build:compile": "tsc -p .",
    "build:bundle": "rollup --config rollup.config.js",
    "lint": "eslint src --ext .ts,vue",
    "format": "npm run prettier:write",
    "prettier:write": "prettier --write \"./src\"",
    "prettier:check": "prettier --check \"./src\"",
    "release": "semantic-release --no-ci"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.esm.js",
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist/"
  ],
  "keywords": [
    "vue",
    "keycloak",
    "composition-api"
  ],
  "license": "Apache-2.0",
  "peerDependencies": {
    "jwt-decode": "^3.1.2",
    "keycloak-js": "20 - 22",
    "vue": "^3.2.45"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^8.0.7",
    "@semantic-release/npm": "^10.0.6",
    "@types/jest": "^29.5.9",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "eslint": "^8.54.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.18.1",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "jwt-decode": "^3.1.2",
    "keycloak-js": "^22.0.5",
    "prettier": "^2.8.8",
    "rimraf": "^3.0.2",
    "rollup": "^2.79.1",
    "semantic-release": "^21.1.2",
    "ts-jest": "^29.1.0",
    "typescript": "^4.9.5",
    "vue": "^3.3.8"
  }
}
