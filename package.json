{
  "name": "@josempgon/vue-keycloak",
  "version": "2.3.0",
  "description": "Keycloak plugin for Vue 3 and Composition API",
  "author": {
    "name": "Gery Hirschfeld",
    "email": "gerhard.hirschfeld@baloise.ch",
    "url": "https://github.com/hirsch88"
  },
  "contributors": [
    "José Miguel Gonçalves <jose.goncalves@inov.pt>"
  ],
  "homepage": "https://github.com/JoseGoncalves/vue-keycloak",
  "repository": {
    "type": "git",
    "url": "git@github.com:JoseGoncalves/vue-keycloak.git"
  },
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watchAll",
    "build": "npm run build:clean && npm run build:compile && npm run build:bundle",
    "build:clean": "rimraf dist && rimraf dist-transpiled",
    "build:compile": "tsc -p .",
    "build:bundle": "rollup --config rollup.config.js",
    "clean": "npm run build:clean && rimraf node_modules",
    "lint": "eslint . --ext .vue,.js,.cjs,.mjs,.ts --ignore-path .gitignore",
    "lint:fix": "npm run lint -- --fix",
    "prettier": "prettier \"**/*.{vue,js,cjs,mjs,ts}\"",
    "format": "npm run prettier -- --write",
    "format:check": "npm run prettier -- --check",
    "release": "semantic-release --no-ci"
  },
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.esm.js",
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist/"
  ],
  "keywords": [
    "vue",
    "keycloak",
    "composition-api"
  ],
  "license": "Apache-2.0",
  "peerDependencies": {
    "keycloak-js": "20 - 23",
    "vue": "^3.2.47"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.7.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^8.1.0",
    "@semantic-release/npm": "^10.0.6",
    "@types/jest": "^29.5.11",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.20.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "keycloak-js": "^23.0.4",
    "prettier": "^3.2.4",
    "rimraf": "^5.0.5",
    "rollup": "^3.29.4",
    "semantic-release": "^21.1.2",
    "ts-jest": "^29.1.1",
    "typescript": "^4.9.5",
    "vue": "^3.4.15"
  }
}
